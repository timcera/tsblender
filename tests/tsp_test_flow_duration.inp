START SETTINGS
#  CONTEXT pest_input
  CONTEXT all
  DATE_FORMAT mm/dd/yyyy
END SETTINGS

###########################################################################
# Process observed and simulated data:
# BLACK EARTH CREEK AT BLACK EARTH (05406500)
###########################################################################

#==> Read in SIMULATED data for BLACK EARTH CREEK AT BLACK EARTH (05406500)
START GET_MUL_SERIES_SSF
  CONTEXT all
  FILE gsflow_sumq_05406500.ssf
  SITE 05406500
  NEW_SERIES_NAME Sq6500i
END GET_MUL_SERIES_SSF

#==> Read in OBSERVED data for BLACK EARTH CREEK AT BLACK EARTH (05406500)
START GET_MUL_SERIES_SSF
  CONTEXT all
  FILE Q_BEC_BE_6500.ssf
  SITE 05406500
  NEW_SERIES_NAME Oq6500i
END GET_MUL_SERIES_SSF

#==> Reduce OBSERVED time-series data for BLACK EARTH CREEK AT BLACK EARTH (05406500)
START REDUCE_TIME_SPAN
  CONTEXT all
  SERIES_NAME Oq6500i
  NEW_SERIES_NAME Oq6500r
  DATE_1 10/01/1990
  DATE_2 09/30/2000
END REDUCE_TIME_SPAN

#==> Clean OBSERVED time-series data for BLACK EARTH CREEK AT BLACK EARTH (05406500)
START SERIES_CLEAN
  CONTEXT all
  SERIES_NAME Oq6500r
  NEW_SERIES_NAME Oq6500
  SUBSTITUTE_VALUE delete
  UPPER_ERASE_BOUNDARY 0.1
END SERIES_CLEAN

#==> Calc new time base: SIMULATED time-series data for BLACK EARTH CREEK AT BLACK EARTH (05406500)
START NEW_TIME_BASE
  CONTEXT all
  SERIES_NAME Sq6500i
  TB_SERIES_NAME Oq6500
  NEW_SERIES_NAME Sq6500
END NEW_TIME_BASE

START FLOW_DURATION
  CONTEXT all
  SERIES_NAME Oq6500
  NEW_TABLE_NAME flow_dur_obs
END FLOW_DURATION

START FLOW_DURATION
  CONTEXT all
  SERIES_NAME Sq6500
  NEW_TABLE_NAME flow_dur_sim
END FLOW_DURATION

START LIST_OUTPUT
  CONTEXT all
  FILE hi_test_flow_duration_obs_output.txt
  G_TABLE_NAME flow_dur_obs
END LIST_OUTPUT

START LIST_OUTPUT
  CONTEXT all
  FILE hi_test_flow_duration_sim_output.txt
  G_TABLE_NAME flow_dur_sim
END LIST_OUTPUT

START WRITE_PEST_FILES
  CONTEXT pest_input
  NEW_PEST_CONTROL_FILE BEC_FLOW_DUR.pst
  AUTOMATIC_USER_INTERVENTION yes
  TEMPLATE_FILE par2par_base.tpl
  MODEL_INPUT_FILE par2par_base.dat
  NEW_INSTRUCTION_FILE sim_vals_flowdur.ins

  OBSERVATION_G_TABLE_NAME flow_dur_obs
  MODEL_G_TABLE_NAME flow_dur_sim
  G_TABLE_WEIGHTS_EQUATION 1.0

  PARAMETER_GROUP_FILE BEC.pest_groups
  PARAMETER_DATA_FILE BEC.pest_parameters
  MODEL_COMMAND_LINE modelcmd.bat

END WRITE_PEST_FILES
